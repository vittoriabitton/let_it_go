{deps,[{supervisor3,".*",
                    {git,"https://github.com/klarna/supervisor3.git",
                         {tag,"1.1.11"}}},
       {kafka_protocol,".*",
                       {git,"https://github.com/klarna/kafka_protocol.git",
                            {tag,"2.3.6"}}}]}.
{edoc_opts,[{preprocess,true},{macros,[{build_brod_cli,true}]}]}.
{erl_opts,[warn_unused_vars,warn_shadow_vars,warn_obsolete_guard,debug_info]}.
{xref_checks,[undefined_function_calls,undefined_functions,locals_not_used,
              deprecated_function_calls,deprecated_functions]}.
{escript_incl_apps,[docopt,brod]}.
{ct_opts,[{enable_builtin_hooks,false}]}.
{cover_enabled,true}.
{cover_opts,[verbose]}.
{cover_export_enabled,true}.
{overrides,[]}.
